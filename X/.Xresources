! Xcursor --------------------------------------------------------------------
Xcursor.theme: DMZ-White
Xcursor.size: 16


! Xft settings ---------------------------------------------------------------
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight


! Terminal settings ----------------------------------------------------------
UXTerm*faceName: Hack
UXTerm*boldFont: Hack Bold
UXTerm*faceSize: 8

UXTerm*fontMenu*fontdefault*Label: Default
UXTerm*loginshell: 1
UXTerm*locale: 1
UXTerm*utf8: 1
UXTerm*utf8Title: 1

UXTerm*scrollttyoutput:false
UXTerm*ScrollTTYKeypress:true

UXTerm*metaSendsEscape: true
UXterm*saveLines: 32768
UXTerm*borderWidth: 0
UXTerm*scrollBar: 0
UXTerm*xftAntialias: 1

